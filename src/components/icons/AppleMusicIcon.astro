---
interface Props {
  class?: string;
  size?: number;
}
const { class: className = '', size = 20 } = Astro.props;
---
<svg
  role="img"
  viewBox="0 0 24 24"
  xmlns="http://www.w3.org/2000/svg"
  width={size}
  height={size}
  fill="currentColor"
  class={className}
  aria-label="Apple Music"
>
  <!-- Rounded square border -->
  <rect x="1" y="1" width="22" height="22" rx="5" fill="none" stroke="currentColor" stroke-width="1.5" opacity="0.35"/>
  <!-- Note heads -->
  <circle cx="6.5" cy="17" r="2.5"/>
  <circle cx="17" cy="13" r="2.5"/>
  <!-- Stems and beam -->
  <path d="M9 17V7l10.5-2v8" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
</svg>
